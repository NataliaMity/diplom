<UserControl x:Class="MityaginaNP.UI.UserControl.UCNotification"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MityaginaNP.UI.UserControl"
             mc:Ignorable="d" 
              Width="200">
    <StackPanel>
        <TextBlock Text="{Binding NotificationType.TypeName}" FontSize="12" />
        <TextBlock Text="{Binding User.LastName}" FontSize="12" />
        <TextBlock Text="{Binding NotificationDateTime}" FontSize="12" />
        <TextBlock Text="{Binding TaskProject.TaskText}" FontSize="12" />
    </StackPanel>
</UserControl>
